<script type="text/javascript">
$(document).ready(function() {
    
    var bizId = $("#ctl00_cntPlaceHolderMain_ProductsBrowser2_hdnBizId").attr("value");

    $.post("/ajax/BusinessLoadProducts.aspx", { skip: $('#divHowManySkip').attr("howMany"), sellOnly: $('#divSellOnly').attr("sellOnly"), bizId: bizId},
               function(data){                    
                    $("#divForProducts").html(data);
        });


    if (parseInt($('#divHowManySkip').attr("howMany")) > parseInt($("#ctl00_cntPlaceHolderMain_ProductsBrowser2_hdnMaxSkip").attr("value")))
    {
        $(".next").hide();            
    }

    $(".prev").hide();

    $(".prev").click(function() {
        $('#divHowManySkip').attr("howMany",parseInt($('#divHowManySkip').attr("howMany"))-1);

        $.post("/ajax/BusinessLoadProducts.aspx", { skip: $('#divHowManySkip').attr("howMany"), sellOnly: $('#divSellOnly').attr("sellOnly"), bizId: bizId},
                   function(data){                    
                       $("#divForProducts").html(data);
            });
        
        $(".next").show();
        if (parseInt($('#divHowManySkip').attr("howMany"))==0)
        {
            $(".prev").hide();
        }
        
    });

    $(".next").click(function() {
        $('#divHowManySkip').attr("howMany",parseInt($('#divHowManySkip').attr("howMany"))+1);
        
        $.post("/ajax/BusinessLoadProducts.aspx", { skip: $('#divHowManySkip').attr("howMany"), sellOnly: $('#divSellOnly').attr("sellOnly"), bizId: bizId},
                   function(data){                    
                       $("#divForProducts").html(data);
            });
        
        $(".prev").show();
        if ((parseInt($('#divHowManySkip').attr("howMany"))+2) > parseInt($("#ctl00_cntPlaceHolderMain_ProductsBrowser2_hdnMaxSkip").attr("value")))
        {
            $(".next").hide();            
        }
        
    });

    $("#aAllProducts").click(function() {
        $('#divHowManySkip').attr("howMany",0);
        $(".prev").hide();
        if (parseInt($('#divHowManySkip').attr("howMany")) > parseInt($("#ctl00_cntPlaceHolderMain_ProductsBrowser2_hdnMaxSkip").attr("value")))
        {
            $(".next").hide();            
        }
        else
        {
            $(".next").show();
        }
        $("#ctl00_cntPlaceHolderMain_ProductsBrowser2_hdnMaxSkip").attr("value", $("#ctl00_cntPlaceHolderMain_ProductsBrowser2_hdnMaxSkipAll").attr("value"));
        $("#divSellOnly").attr("sellOnly",0);
        $(this).addClass("active");
        $("#aSellOnlyProducts").removeClass("active");
        $.post("/ajax/BusinessLoadProducts.aspx", { skip: $('#divHowManySkip').attr("howMany"), sellOnly: $('#divSellOnly').attr("sellOnly"), bizId: bizId},
                   function(data){                    
                       $("#divForProducts").html(data);
            });
    });

    $("#aSellOnlyProducts").click(function() {
        $('#divHowManySkip').attr("howMany",0);
         $(".prev").hide();
                 if (parseInt($('#divHowManySkip').attr("howMany")) > parseInt($("#ctl00_cntPlaceHolderMain_ProductsBrowser2_hdnMaxSkip").attr("value")))
        {
            $(".next").hide();            
        }
        else
        {
            $(".next").show();
        }
        $("#ctl00_cntPlaceHolderMain_ProductsBrowser2_hdnMaxSkip").attr("value", $("#ctl00_cntPlaceHolderMain_ProductsBrowser2_hdnMaxSkipSellOnly").attr("value"));
        $("#divSellOnly").attr("sellOnly",1);
        $(this).addClass("active");
        $("#aAllProducts").removeClass("active");
        $.post("/ajax/BusinessLoadProducts.aspx", { skip: $('#divHowManySkip').attr("howMany"), sellOnly: $('#divSellOnly').attr("sellOnly"), bizId: bizId},
                   function(data){                    
                       $("#divForProducts").html(data);
            });
    });
    

 });
</script>
