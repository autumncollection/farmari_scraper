<br><br>
<div align="center">
	<script type="text/javascript">
	var months = ["leden 2017","únor","březen","duben","květen","červen","červenec","srpen","září","říjen","listopad","prosinec 2017","leden 2018","únor"];
	var daycounts = [31,28,31,30,31,30,31,31,30,31,30,31,31,28]; //počet dnů v měsících - leden17-únor18
	var firstdays = [6,2,2,5,0,3,5,1,4,6,2,4,6,2]; //počet prázdných polí v týdnech pro 1/2017-2/2018
	var dCurrent = new Date();
	var m = dCurrent.getMonth();
	var y = dCurrent.getFullYear();
  var apps = [
/* od,do,dnes,měsíc,zpráva */
//		[21,3,21,3, "BioSad, Předjarní seminář, Tetčice"],
		[4,4,4,4, "OUČR, SISPO, Valná hromada SISPO, Milovice u Hořic"],
		[5,4,5,4, "OUČR, Valná hromada OUČR, Holovousy"],
		[26,4,27,4, "OUČR, OUMS, Mrazy a jejich dopady, Hrubá Voda"]
		];

	function CheckDate(month,dayno)
	{
	var retval = new String(dayno);
	var m = month + 1;
	for(var app = 0; app < apps.length; app++)
	{
	if(m == apps[app][1] ) //první měsíc
	{
	if(apps[app][3] - apps[app][1] > 0)
	{
	if(dayno >= apps[app][0])
	{
	retval = "<div class='hol' title='" + apps[app][4] + "'>" + dayno + "</div>";
	}
	}
	else
	{
	if(dayno >= apps[app][0] && dayno <= apps[app][2])
	{
	retval = "<div class='hol' title='" + apps[app][4] + "'>" + dayno + "</div>";
	}
	}
	}
	else if(m == apps[app][3]) // další měsíc
	{
	if(dayno <= apps[app][2])
	{
	retval = "<div class='hol' title='" + apps[app][4] + "'>" + dayno + "</div>";
	}
	}
	else if( m > apps[app][1] && m < apps[app][3] )
	{
	retval = "<div class='hol' title='" + apps[app][4] + "'>" + dayno + "</div>";
	}
	}
	return retval;
	}
	function PrintMonth(month)
	{
	var done = false;
	var day = 0;
	var yn = y+1;
	document.writeln("<center><FONT style='font-family: comic sans ms; color: yellow; width: 93%; padding: 2px; font-weight: bold; font-size: 11pt'>"+months[month]+"</FONT></center>"); //bez roku - ""

	document.writeln("<TABLE class='calend' CELLPADDING='1' CELLSPACING='1'><TR><TH>Po</TH><TH>Út</TH><TH>St</TH><TH>Čt</TH><TH>Pá</TH><TH>So</TH><TH>Ne</TH></TR>");

while(!done)
{
document.write("<tr style='font-size: 8pt; text-align: center; font-weight: bold;'>");

function PrintWeek(monthno,start,min,max)
	{
	var d;
	var desc;
	for(var j = 0; j < 7; j++)
	{
	document.write("<td>");
	d = start + j;
	if(d >= min && d < max + min)
	{
	desc = CheckDate(monthno,d - min + 1);
	document.write(desc);
	}
	document.write("</td>");
	}
	}

PrintWeek(month,day, firstdays[month], daycounts[month]);
document.write("</tr>");
day = day + 7;
if( day > daycounts[month] + firstdays[month])
{
done = true;
}
}

document.write("</table>");
}

	</script>

	<table>
		<tr>
			<td>
			<script type="text/javascript">
			PrintMonth(m);
			</script>
			</td>
		</tr>
		<tr>
			<td>
			<script type="text/javascript">
   		PrintMonth(m+1);
			</script>
			</td>
		</tr>
	</table>
</div>
